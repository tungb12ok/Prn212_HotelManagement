<Window x:Class="WindowsPresentation.CustomerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:WindowsPresentation"
        mc:Ignorable="d"
        Title="Customer Window" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button Content="Manage Profile" Click="ManageProfile_Click" Margin="10"/>
            <Button Content="View Booking History" Click="ViewBookingHistory_Click" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
