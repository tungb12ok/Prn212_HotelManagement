<Window x:Class="WindowsPresentation.ProfileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:WindowsPresentation"
        mc:Ignorable="d"
        Title="Manage Profile" Height="400" Width="400">
    <Grid>
        <StackPanel>
            <Label Content="Full Name:" Margin="5"/>
            <TextBox x:Name="FullNameTextBox" Margin="5"/>

            <Label Content="Telephone:" Margin="5"/>
            <TextBox x:Name="TelephoneTextBox" Margin="5"/>

            <Label Content="Email:" Margin="5"/>
            <TextBox x:Name="EmailTextBox" Margin="5"/>

            <Label Content="Password:" Margin="5"/>
            <PasswordBox x:Name="PasswordBox" Margin="5"/>

            <Label Content="Birthday:" Margin="5"/>
            <DatePicker x:Name="BirthdayDatePicker" Margin="5"/>

            <Button Content="Save Changes" Click="SaveChanges_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
